{
  "diagnostic.infoSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.errorSign": "•",
  "diagnostic.hintSign": "•",
  "suggest.timeout": 5000,
  "git.addedSign.text": "▌",
  "git.changedSign.text": "▌",
  "git.changeRemovedSign.text": "▌",
  "git.removedSign.text": "◢",
  "git.topRemovedSign.text": "◥",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.realtimeGutters": false,
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "eslint.filetypes": [
    "javascript", 
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": "~/.config/yarn/global/node_modules",
  "snippets.priority": 999,
  "codeLens.enable": true,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.prospectorEnabled": true,
  "python.linting.prospectorArgs": [],
  "python.linting.mypyEnabled": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "prettier.disableSuccessMessage": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.semi": false,
  "prettier.onlyUseLocalVersion": true,
  "yaml.schemas": {
    "http://json.schemastore.org/composer": "composer.yaml",
    "http://json.schemastore.org/gitlab-ci": ".gitlab-ci.yml",
    "https://gist.githubusercontent.com/JoosepAlviste/5e806ca470e37eb442b839945d9ef359/raw/9b8f10cdaa5537822338f35a48902c5ef21a5068/schema-1.0.0.json": "tg-project.yaml"
  },
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "coc.preferences.colorSupport": false
}
